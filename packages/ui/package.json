{
  "name": "camino-ui",
  "version": "1.0.0",
  "description": "Interface web de Camino, le cadastre minier num√©rique ouvert",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && vite build --emptyOutDir",
    "dev": "vite --host",
    "dev:update": "npm-check-updates && npm install && npm audit fix",
    "start": "node ./index.js",
    "test": "vitest --environment jsdom --root src/",
    "lint": "prettier --write src && eslint --fix --ext .js,.ts,.tsx src --max-warnings=0",
    "lint:check": "prettier --check src && eslint --ext .js,.ts,.tsx src --max-warnings=0",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "generate-icon-types": "ts-node --transpile-only scripts/generateIconTypes.ts"
  },
  "overrides": {
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@gouvfr/dsfr": "^1.10.2",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@sentry/browser": "^7.112.2",
    "@sentry/tracing": "^7.112.2",
    "@sentry/vue": "^7.112.2",
    "@storybook/addon-actions": "8.0.9",
    "@storybook/addon-controls": "8.0.9",
    "@storybook/builder-vite": "8.0.9",
    "@storybook/preview-api": "8.0.9",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "8.0.9",
    "@storybook/vue3-vite": "8.0.9",
    "@testing-library/vue": "^8.0.3",
    "@types/file-saver": "^2.0.7",
    "@types/geojson": "^7946.0.14",
    "@types/leaflet": "^1.9.12",
    "@types/leaflet.markercluster": "^1.5.4",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@uppy/core": "^3.10.1",
    "@uppy/tus": "^3.5.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.5.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-graphql-tag": "^3.3.0",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "babel-preset-vite": "^1.1.3",
    "camino-common": "1.0.0",
    "chart.js": "^4.4.2",
    "core-js": "^3.37.0",
    "dotenv": "^16.4.5",
    "email-validator": "^2.0.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "file-saver": "^2.0.5",
    "graphql": "^16.8.1",
    "graphql-react": "^20.0.0",
    "graphql-tag": "^2.12.6",
    "jsdom": "^24.0.0",
    "jsondiffpatch-rc": "^0.4.2",
    "leaflet": "^1.9.4",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-gesture-handling": "^1.2.2",
    "leaflet.markercluster": "^1.5.3",
    "maplibre-gl": "^4.1.3",
    "postcss": "^8.4.38",
    "postcss-custom-media": "^10.0.4",
    "postcss-preset-env": "^9.5.9",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "8.0.9",
    "storybook-vue3-router": "^5.0.0",
    "tus-js-client": "^4.1.0",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.2.10",
    "vitest": "^1.5.2",
    "vue": "^3.4.26",
    "vue-router": "^4.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "localStorage": "readonly",
      "HTMLOrSVGElement": "readonly",
      "JSX": "readonly",
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "T": "readonly",
      "K": "readonly",
      "P": "readonly",
      "dsfr": "readonly"
    },
    "extends": [
      "standard",
      "prettier"
    ],
    "ignorePatterns": [
      "!.storybook"
    ],
    "rules": {
      "camelcase": 0,
      "no-unused-vars": 0,
      "no-use-before-define": 0,
      "@typescript-eslint/ban-ts-comment": 0
    },
    "overrides": [
      {
        "parserOptions": {
          "project": true
        },
        "files": [
          "*.tsx",
          "*.ts",
          "*.js"
        ],
        "extends": [
          "plugin:@typescript-eslint/recommended"
        ],
        "rules": {
          "camelcase": 0,
          "newline-before-return": 0,
          "no-use-before-define": 0,
          "no-redeclare": 0,
          "no-return-await": "error",
          "default-param-last": 0,
          "no-unused-vars": 0,
          "@typescript-eslint/explicit-function-return-type": 0,
          "@typescript-eslint/explicit-module-boundary-types": 0,
          "@typescript-eslint/no-empty-interface": 0,
          "@typescript-eslint/no-explicit-any": 0,
          "@typescript-eslint/no-non-null-assertion": 0,
          "@typescript-eslint/no-var-requires": 0,
          "@typescript-eslint/ban-ts-comment": 0,
          "@typescript-eslint/no-unused-vars": [
            "error",
            {
              "varsIgnorePattern": "^_",
              "argsIgnorePattern": "^_"
            }
          ],
          "@typescript-eslint/no-empty-function": 0,
          "@typescript-eslint/strict-boolean-expressions": "error"
        }
      }
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "bracketSpacing": true,
    "trailingComma": "es5",
    "printWidth": 200
  },
  "postcss": {
    "plugins": {
      "postcss-custom-media": {},
      "postcss-preset-env": {
        "stage": 0
      }
    }
  },
  "browserslist": [
    "supports es6-module"
  ],
  "dependencies": {
    "zod": "^3.23.4"
  }
}
