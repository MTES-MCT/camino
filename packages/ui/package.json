{
  "name": "camino-ui",
  "version": "1.0.0",
  "description": "Interface web de Camino, le cadastre minier numérique ouvert",
  "private": true,
  "scripts": {
    "build": "vite build --emptyOutDir",
    "dev": "vite",
    "dev:update": "npm-check-updates && npm install && npm audit fix",
    "start": "node ./index.js",
    "test": "cross-env NODE_ENV=test && jest --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --watch --verbose",
    "lint": "prettier --write src && eslint --fix --ext .js,.ts,.vue src",
    "lint:check": "prettier --check src && eslint src",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "@comment dependencies": {
    "eslint": "eslint et eslint-plugin-vue, bloqués à 0.7.x https://github.com/vuejs/eslint-config-standard/issues/10",
    "typescript": "bloqué à 4.4.x https://github.com/xjamundx/eslint-plugin-promise/issues/218",
    "graphql-react": "faut configurer Jest pour qu’il puisse interpréter les .mjs",
    "jest": "jest, jest-util, ts-jest et babel-jest bloqués en 27.x.x https://github.com/vuejs/vue-jest/issues/467",
    "@mdx-js/preact": "dépendance transitive à @storybook/builder-vite -> https://github.com/storybookjs/builder-vite/issues/406"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.17.10",
    "@juggle/resize-observer": "^3.3.1",
    "@mdx-js/preact": "^1.6.22",
    "@rollup/plugin-inject": "^4.0.4",
    "@sentry/browser": "^6.19.7",
    "@sentry/integrations": "^6.19.7",
    "@storybook/addon-actions": "^6.5.7",
    "@storybook/addon-controls": "^6.5.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storyshots": "^6.5.7",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/vue3": "^6.5.7",
    "@types/jest": "^27.5.0",
    "@types/leaflet": "^1.7.9",
    "@types/leaflet.markercluster": "^1.4.6",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/parser": "^5.22.0",
    "@uppy/core": "^2.1.10",
    "@uppy/tus": "^2.2.2",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/vue3-jest": "27.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-graphql-tag": "^3.3.0",
    "babel-preset-vite": "^1.0.4",
    "chalk": "^5.0.1",
    "chart.js": "^3.7.1",
    "commitizen": "^4.2.4",
    "core-js": "^3.22.4",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "cz-conventional-changelog": "^3.3.0",
    "email-validator": "^2.0.4",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "file-saver": "^2.0.5",
    "graphql": "^16.4.0",
    "graphql-react": "^15.0.0",
    "graphql-tag": "^2.12.6",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-util": "^27.5.1",
    "json2csv": "^5.0.7",
    "jsondiffpatch": "^0.4.1",
    "leaflet": "1.8.0",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-gesture-handling": "^1.2.2",
    "leaflet-markers-canvas": "^0.2.2",
    "leaflet.markercluster": "^1.5.3",
    "npm-check-updates": "^12.5.11",
    "postcss": "^8.4.14",
    "postcss-calc": "^8.2.4",
    "postcss-import": "14.1.0",
    "postcss-inline-svg": "5.0.0",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.5.0",
    "prettier": "^2.6.2",
    "rbush": "^3.0.1",
    "snarkdown": "^2.0.0",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.4",
    "vite": "^2.9.7",
    "vue": "^3.2.33",
    "vue-loader": "^17.0.0",
    "vue-meta": "^2.4.0",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2",
    "vuex-router-sync": "^6.0.0-rc.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest/globals": true
    },
    "globals": {
      "defineProps": "readonly",
      "defineEmits": "readonly"
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "standard",
      "prettier"
    ],
    "rules": {
      "vue/no-mutating-props": 0,
      "vue/valid-v-bind-sync": 0,
      "vue/no-multiple-template-root": [
        "warn"
      ],
      "vue/attribute-hyphenation": [
        "error",
        "never"
      ],
      "vue/v-on-event-hyphenation": 0,
      "no-unused-vars": 0,
      "vue/multi-word-component-names": 0
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "plugins": [
      "jest"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-normalize": {},
      "postcss-inline-svg": {},
      "postcss-custom-media": {},
      "postcss-preset-env": {
        "stage": 0
      },
      "postcss-calc": {}
    }
  },
  "browserslist": [
    "supports es6-module"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
