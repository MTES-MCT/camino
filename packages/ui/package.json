{
  "name": "camino-ui",
  "version": "1.0.0",
  "description": "Interface web de Camino, le cadastre minier num√©rique ouvert",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "dev": "vite",
    "dev:update": "npm-check-updates && npm install && npm audit fix",
    "start": "node ./index.js",
    "test": "vitest --environment jsdom --root src/",
    "lint": "prettier --write src && eslint --fix --ext .js,.ts,.vue src",
    "lint:check": "prettier --check src && eslint src",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "generate-icon-types": "ts-node --transpile-only scripts/generateIconTypes.ts"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@rollup/plugin-inject": "^4.0.4",
    "@sentry/tracing": "^7.11.1",
    "@sentry/vue": "^7.11.1",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-controls": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/testing-vue3": "^0.0.2",
    "@storybook/vue3": "^6.5.13",
    "@testing-library/vue": "^6.6.1",
    "@types/file-saver": "^2.0.5",
    "@types/leaflet": "^1.7.11",
    "@types/leaflet.markercluster": "^1.5.1",
    "@typescript-eslint/parser": "^5.34.0",
    "@uppy/core": "^3.0.4",
    "@uppy/tus": "^3.0.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vitest/coverage-c8": "^0.25.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-graphql-tag": "^3.3.0",
    "babel-preset-vite": "^1.0.4",
    "chart.js": "^3.9.1",
    "core-js": "^3.24.1",
    "dotenv": "^16.0.3",
    "email-validator": "^2.0.4",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.4.0",
    "file-saver": "^2.0.5",
    "graphql": "^16.6.0",
    "graphql-react": "^20.0.0",
    "graphql-tag": "^2.12.6",
    "jsondiffpatch": "^0.4.1",
    "leaflet": "1.8.0",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-gesture-handling": "^1.2.2",
    "leaflet.markercluster": "^1.5.3",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.8.0",
    "typescript": "^4.8.4",
    "typescript-plugin-css-modules": "^4.1.1",
    "vite": "^3.2.4",
    "vitest": "^0.25.2",
    "vitest-fetch-mock": "^0.2.1",
    "vue": "^3.2.45",
    "vue-loader": "^17.0.1",
    "vue-meta": "^2.4.0",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9",
    "vuex": "^4.1.0",
    "vuex-router-sync": "^6.0.0-rc.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "JSX": "readonly",
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "T": "readonly",
      "K": "readonly",
      "P": "readonly"
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "standard",
      "prettier"
    ],
    "rules": {
      "vue/no-mutating-props": 0,
      "vue/valid-v-bind-sync": 0,
      "vue/no-multiple-template-root": [
        "error"
      ],
      "vue/attribute-hyphenation": [
        "error",
        "never"
      ],
      "vue/v-on-event-hyphenation": 0,
      "no-unused-vars": 0,
      "vue/multi-word-component-names": 0
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "postcss": {
    "plugins": {
      "postcss-custom-media": {},
      "postcss-preset-env": {
        "stage": 0
      }
    }
  },
  "browserslist": [
    "supports es6-module"
  ]
}
