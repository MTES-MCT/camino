{
  "name": "camino-ui",
  "version": "1.0.0",
  "description": "Interface web de Camino, le cadastre minier num√©rique ouvert",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "dev": "vite --host",
    "dev:update": "npm-check-updates && npm install && npm audit fix",
    "start": "node ./index.js",
    "test": "vitest --environment jsdom --root src/",
    "lint": "prettier --write src && eslint --fix --ext .js,.ts,.tsx,.vue src",
    "lint:check": "prettier --check src && eslint src",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "generate-icon-types": "ts-node --transpile-only scripts/generateIconTypes.ts"
  },
  "overrides": {
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@gouvfr/dsfr": "^1.10.2",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@sentry/tracing": "^7.56.0",
    "@sentry/vue": "^7.56.0",
    "@storybook/addon-actions": "^7.0.22",
    "@storybook/addon-controls": "^7.0.22",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/builder-vite": "^7.0.22",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/testing-vue3": "^1.0.0",
    "@storybook/vue3": "^7.0.22",
    "@storybook/vue3-vite": "^7.0.22",
    "@testing-library/vue": "^7.0.0",
    "@types/file-saver": "^2.0.5",
    "@types/leaflet": "^1.9.3",
    "@types/leaflet.markercluster": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@uppy/core": "^3.2.1",
    "@uppy/tus": "^3.1.1",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-graphql-tag": "^3.3.0",
    "babel-preset-vite": "^1.1.0",
    "chart.js": "^4.3.0",
    "core-js": "^3.31.0",
    "dotenv": "^16.3.1",
    "email-validator": "^2.0.4",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.15.0",
    "file-saver": "^2.0.5",
    "graphql": "^16.6.0",
    "graphql-react": "^20.0.0",
    "graphql-tag": "^2.12.6",
    "jsdom": "^22.1.0",
    "jsondiffpatch-rc": "^0.4.2",
    "leaflet": "^1.9.4",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-gesture-handling": "^1.2.2",
    "leaflet.markercluster": "^1.5.3",
    "maplibre-gl": "^3.6.2",
    "postcss": "^8.4.24",
    "postcss-preset-env": "^8.5.0",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.3",
    "storybook": "^7.0.22",
    "storybook-vue3-router": "^3.0.0",
    "typescript": "^5.3.2",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^5.0.2",
    "vitest": "^0.34.6",
    "vitest-fetch-mock": "^0.2.2",
    "vue": "^3.3.4",
    "vue-loader": "^17.2.2",
    "vue-meta": "^2.4.0",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.8.1",
    "vuex": "^4.1.0",
    "vuex-router-sync": "^6.0.0-rc.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "globals": {
      "HTMLOrSVGElement": "readonly",
      "JSX": "readonly",
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "T": "readonly",
      "K": "readonly",
      "P": "readonly",
      "dsfr": "readonly"
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "standard",
      "prettier"
    ],
    "ignorePatterns": ["!.storybook"],
    "rules": {
      "camelcase": 0,
      "vue/no-mutating-props": 0,
      "vue/valid-v-bind-sync": 0,
      "vue/no-multiple-template-root": [
        "error"
      ],
      "vue/attribute-hyphenation": [
        "error",
        "never"
      ],
      "vue/v-on-event-hyphenation": 0,
      "no-unused-vars": 0,
      "vue/multi-word-component-names": 0,
      "vue/one-component-per-file": 0,
      "no-use-before-define": 0,
      "@typescript-eslint/ban-ts-comment": 0
    },
    "overrides": [
      {
        "parserOptions": {
          "project": true
        },
        "files": [
          "*.tsx"
        ],
        "extends": [
          "plugin:@typescript-eslint/recommended"
        ],
        "rules": {
          "vue/require-prop-types": 0,
          "vue/require-explicit-emits": 0,
          "camelcase": 0,
          "newline-before-return": 1,
          "no-use-before-define": 0,
          "no-redeclare": 0,
          "no-return-await": "error",
          "default-param-last": 0,
          "no-unused-vars": 0,
          "@typescript-eslint/explicit-function-return-type": 0,
          "@typescript-eslint/explicit-module-boundary-types": 0,
          "@typescript-eslint/no-empty-interface": 0,
          "@typescript-eslint/no-explicit-any": 0,
          "@typescript-eslint/no-non-null-assertion": 0,
          "@typescript-eslint/no-var-requires": 0,
          "@typescript-eslint/ban-ts-comment": 0,
          "@typescript-eslint/no-unused-vars": [
            "error",
            {
              "varsIgnorePattern": "^_",
              "argsIgnorePattern": "^_"
            }
          ],
          "@typescript-eslint/no-empty-function": 0,
          "@typescript-eslint/strict-boolean-expressions": "warn"
        }
      }
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "bracketSpacing": true,
    "trailingComma": "es5",
    "printWidth": 200
  },
  "postcss": {
    "plugins": {
      "postcss-custom-media": {},
      "postcss-preset-env": {
        "stage": 0
      }
    }
  },
  "browserslist": [
    "supports es6-module"
  ],
  "dependencies": {
    "zod": "^3.22.4"
  }
}
